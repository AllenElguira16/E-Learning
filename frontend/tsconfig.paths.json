{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~components": [
        "src/components"
      ],
      "~helpers": [
        "src/helpers"
      ],
      "~pages": [
        "src/pages"
      ],
      "~store": [
        "src/store"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~helpers/*": [
        "src/helpers/*"
      ],
      "~pages/*": [
        "src/pages/*"
      ],
      "~store/*": [
        "src/store/*"
      ]
    }
  }
}